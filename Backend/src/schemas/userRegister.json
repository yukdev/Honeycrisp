{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "minLength": 6,
      "maxLength": 30,
      "format": "email"
    },
    "password": {
      "type": "string",
      "minLength": 5,
      "maxLength": 20
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    }
  },
  "additionalProperties": false,
  "required": ["email", "password", "name"]
}
